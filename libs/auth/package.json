{
  "name": "@rckit/auth",
  "version": "4.34.1",
  "description": "RCKIT – Auth – React authentication management with session persistence",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            ycmd dev",
    "build": "          ycmd build",
    "test": "           ycmd test",
    "prepack": "        ycmd prepack",
    "release": "        ycmd release"
  },
  "dependencies": {
    "@lsk4/algos": "^4.5.0",
    "@lsk4/env": "^4.6.0",
    "@lsk4/err": "^4.5.0",
    "@lsk4/log": "^4.8.0",
    "@rckit/api-client": "workspace:^",
    "@rckit/debug": "workspace:^",
    "@rckit/form": "workspace:^",
    "@rckit/link": "workspace:^",
    "fishbird": "^1.1.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.2",
    "use-local-storage-state": "^18.3.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-bootstrap": "*",
    "use-local-storage-state": "*"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lsk4/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lsk4/eslint-config"
  },
  "files": [
    "cjs",
    "lib",
    "README.md",
    "LICENCE"
  ],
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./cjs/index.d.ts",
      "default": "./cjs/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./cjs/*.d.ts",
      "default": "./cjs/*.js"
    }
  },
  "_size-limit": [
    {
      "path": "lib/index.js",
      "limit": "10kb",
      "ignore": [
        "util",
        "timers",
        "crypto"
      ]
    }
  ],
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": {
    "type": "git",
    "url": "https://github.com/rckit/RCKIT.git",
    "directory": "libs/auth"
  },
  "homepage": "https://github.com/rckit/RCKIT/tree/main/libs/auth",
  "bugs": "https://github.com/rckit/RCKIT/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "////": "/////////========================/////////========================/////////========================/////////"
}

{
  "name": "@rckit/form",
  "version": "4.34.0",
  "description": "RCKIT – Form – React form components and hooks built on react-hook-form and react-bootstrap",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            ycmd dev",
    "build": "          ycmd build",
    "test": "           ycmd test",
    "prepack": "        ycmd prepack",
    "release": "        ycmd release"
  },
  "dependencies": {
    "@lsk4/env": "^4.6.0",
    "@lsk4/err": "^4.5.0",
    "@lsk4/log": "^4.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.2",
    "react-hook-form": "^7.49.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-bootstrap": "*",
    "react-hook-form": "*"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lsk4/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lsk4/eslint-config"
  },
  "files": [
    "cjs",
    "lib",
    "README.md",
    "LICENCE"
  ],
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./cjs/index.d.ts",
      "default": "./cjs/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./cjs/*.d.ts",
      "default": "./cjs/*.js"
    }
  },
  "_size-limit": [
    {
      "path": "lib/index.js",
      "limit": "10kb",
      "ignore": [
        "util",
        "timers",
        "crypto"
      ]
    }
  ],
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": {
    "type": "git",
    "url": "https://github.com/rckit/RCKIT.git",
    "directory": "libs/form"
  },
  "homepage": "https://github.com/rckit/RCKIT/tree/main/libs/form",
  "bugs": "https://github.com/rckit/RCKIT/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "////": "/////////========================/////////========================/////////========================/////////"
}
